<h4>Fixtures</h4>
<h5>Gameweek: <%= @week %></h5>
<table>
  <thead>
    <th>
      Home
    </th>
    <th>
    </th>
    <th>
      Away
    </th>
  </thead>
  <tbody>
    <%= form_with model: current_user do |f| %>
      <% @fixture&.matches.each do |match| %>
	<tr>
	  <td>
	    <%= render partial: "fixture_button", locals: {team: match['home_team']['name'], f: f}%>
	  </td>
	  <td>
	    vs
	  </td>
	  <td>
	    <%= render partial: "fixture_button", locals: {team: match['away_team']['name'], f: f}%>
	  </td>
	</tr>
      <% end %>
    <% end %>
  </tbody>
</table>
<p>Last Updated: <%= @fixture&.updated_at %></p>
